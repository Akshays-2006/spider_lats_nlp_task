       masked_attention_weights=torch.softmax(masked_attention_score,dim=-1)\n        context_vector=masked_attention_weights @ value\n\n        return masked_attention_weights,context_vector","metadata":{"_uuid":"8f2839f25d086af736a60e9eeb907d3b93b6e0e5","_cell_guid":"b1076dfc-b9ad-4769-8c92-a6c4dae69d19","trusted":true,"execution":{"iopub.status.busy":"2025-12-28T15:50:56.358711Z","iopub.execute_input":"2025-12-28T15:50:56.359111Z","iopub.status.idle":"2025-12-28T15:50:56.365613Z","shell.execute_reply.started":"2025-12-28T15:50:56.359083Z","shell.execute_reply":"2025-12-28T15:50:56.364915Z"}},"outputs":[],"execution_count":2},{"cell_type":"markdown","source":"## Multi Head Attention","metadata":{}},{"cell_type":"code","source":"class MHA(nn.Module):\n    def __init__(self,d_model,num_heads,dropout=0.5,bias=False):\n        super().__init__()\n\n        assert d_model%num_heads==0, \"Model dimensions should be a multiple of number of heads\"\n        \n        self.d_model=d_model\n        self.num_heads=num_heads\n        self.W_query=nn.Linear(d_model,d_model,bias)\n        self.W_key=nn.Linear(d_model,d_model,bias)\n        self.W_value=nn.Linear(d_model,d_model,bias)\n        self.d_k=d_model//num_heads\n        self.W_O=nn.Linear(d_model,d_model,bias)\n        self.dropout=nn.Dropout(p=dropout)\n\n    def mask(self,context_length,device):\n        mask=torch.triu(torch.ones(context_length,context_length,device=device),diagonal=1)\n        return mask\n        \n    def msa(self,query,key,value):\n            attention_score=query @ key.transpose(-2,-1)\n            attention_score=attention_score/(self.d_k**0.5)\n            mask=self.mask(query.shape[-2],query.device).unsqueeze(0).unsqueeze(0)\n            masked_attention_score=attention_score.masked_fill(mask.bool(),-torch.inf)\n            masked_attention_weights=torch.softmax(masked_attention_score,dim=-1)\n            #masked_attention_weights=self.dropout(masked_attention_weights)\n            context_vector=masked_attention_weights @ value\n    \n            return masked_attention_weights,context_vector\n        \n    def split_heads(self,x):\n        batch_size,seq_length,d_model=x.size()\n        return x.view(batch_size,seq_length,self.num_heads,self.d_k).transpose(1, 2)\n        \n    def combine_heads(self,x):\n        batch_size,num_heads,seq_length,d_k=x.size()\n        return x.transpose(1, 2).contiguous().view(batch_size,seq_length,self.d_model)\n\n    def forward(self,input):\n        query=self.split_heads(self.W_query(input))\n        key=self.split_heads(self.W_key(input))\n        value=self.split_heads(self.W_value(input))\n        \n        masked_attention_weights,attention_output=self.msa(query,key,value)\n        output=self.W_O(self.combine_heads(attention_output))\n        return masked_attention_weights,output","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-12-28T15:50:58.506861Z","iopub.execute_input":"2025-12-28T15:50:58.507608Z","iopub.status.idle":"2025-12-28T15:50:58.516511Z","shell.execute_reply.started":"2025-12-28T15:50:58.507578Z","shell.execute_reply":"2025-12-28T15:50:58.515673Z"}},"outputs":[],"execution_count":3},{"cell_type":"code","source":"mha=MHA(d_model=728,num_heads=4)\nmha","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-12-28T15:51:00.345768Z","iopub.execute_input":"2025-12-28T15:51:00.346483Z","iopub.status.idle":"2025-12-28T15:51:00.384163Z","shell.execute_reply.started":"2025-12-28T15:51:00.346453Z","shell.execute_reply":"2025-12-28T15:51:00.383622Z"}},"outputs":[{"execution_count":4,"output_type":"execute_result","data":{"text/plain":"MHA(\n  (W_query): Linear(in_features=728, out_features=728, bias=False)\n  (W_key): Linear(in_features=728, out_features=728, bias=False)\n  (W_value): Linear(in_features=728, out_features=728, bias=False)\n  (W_O): Linear(in_features=728, out_features=728, bias=False)\n  (dropout): Dropout(p=0.5, inplace=False)\n)"},"metadata":{}}],"execution_cou